        <FIELD NAME="goal" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="time" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="userid_key" UNIQUE="false" FIELDS="userid"/>
        <INDEX NAME="startlevel_key" UNIQUE="false" FIELDS="startlevel"/>
        <INDEX NAME="currentlevel_key" UNIQUE="false" FIELDS="currentlevel"/>
        <INDEX NAME="readerid_key" UNIQUE="false" FIELDS="readerid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="reader_messages" COMMENT="Messages displayed on Reader module view page">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="readerid" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="teacherid" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="groupids" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="message" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="timefinish" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="readerid_key" UNIQUE="false" FIELDS="readerid"/>
        <INDEX NAME="timefinish_key" UNIQUE="false" FIELDS="timefinish"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="reader_question_instances" COMMENT="reader_question_instances table retrofitted from MySQL">
      <FIELDS>
