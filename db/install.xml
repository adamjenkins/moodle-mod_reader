<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/reader/db" VERSION="20140402" COMMENT="Reader Module tables"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="reader" COMMENT="Main information about each reader" NEXT="reader_attempts">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="true" NEXT="course"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="name" PREVIOUS="id"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" NEXT="intro" PREVIOUS="course"/>
        <FIELD NAME="intro" TYPE="text" NOTNULL="false" SEQUENCE="false" NEXT="introformat" PREVIOUS="name"/>
        <FIELD NAME="introformat" TYPE="int" LENGTH="4" NOTNULL="true" DEFAULT="0" SEQUENCE="false" NEXT="timeopen" PREVIOUS="intro"/>
        <FIELD NAME="timeopen" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="timeclose" PREVIOUS="introformat"/>
        <FIELD NAME="timeclose" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="optionflags" PREVIOUS="timeopen"/>
        <FIELD NAME="optionflags" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="penaltyscheme" PREVIOUS="timeclose"/>
        <FIELD NAME="penaltyscheme" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="attempts" PREVIOUS="optionflags"/>
        <FIELD NAME="attempts" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="attemptonlast" PREVIOUS="penaltyscheme"/>
        <FIELD NAME="attemptonlast" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="grademethod" PREVIOUS="attempts"/>
        <FIELD NAME="grademethod" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="1" SEQUENCE="false" NEXT="decimalpoints" PREVIOUS="attemptonlast"/>
        <FIELD NAME="decimalpoints" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="2" SEQUENCE="false" NEXT="review" PREVIOUS="grademethod"/>
        <FIELD NAME="review" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="questionsperpage" PREVIOUS="decimalpoints"/>
        <FIELD NAME="questionsperpage" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="1" SEQUENCE="false" NEXT="shufflequestions" PREVIOUS="review"/>
        <FIELD NAME="shufflequestions" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="shuffleanswers" PREVIOUS="questionsperpage"/>
        <FIELD NAME="shuffleanswers" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="1" SEQUENCE="false" NEXT="questions" PREVIOUS="shufflequestions"/>
        <FIELD NAME="questions" TYPE="text" NOTNULL="false" SEQUENCE="false" NEXT="sumgrades" PREVIOUS="shuffleanswers"/>
        <FIELD NAME="sumgrades" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="grade" PREVIOUS="questions"/>
        <FIELD NAME="grade" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="usecourse" PREVIOUS="sumgrades"/>
        <FIELD NAME="usecourse" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="timecreated" PREVIOUS="grade"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="timemodified" PREVIOUS="usecourse"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="timelimit" PREVIOUS="timecreated"/>
        <FIELD NAME="timelimit" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="15" SEQUENCE="false" NEXT="password" PREVIOUS="timemodified"/>
        <FIELD NAME="password" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" NEXT="subnet" PREVIOUS="timelimit"/>
        <FIELD NAME="subnet" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" NEXT="popup" PREVIOUS="password"/>
        <FIELD NAME="popup" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="individualstrictip" PREVIOUS="subnet"/>
        <FIELD NAME="individualstrictip" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="delay1" PREVIOUS="popup"/>
        <FIELD NAME="delay1" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="delay2" PREVIOUS="individualstrictip"/>
        <FIELD NAME="delay2" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="percentforreading" PREVIOUS="delay1"/>
        <FIELD NAME="percentforreading" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="60" SEQUENCE="false" NEXT="nextlevel" PREVIOUS="delay2"/>
        <FIELD NAME="nextlevel" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="6" SEQUENCE="false" NEXT="quizpreviouslevel" PREVIOUS="percentforreading"/>
        <FIELD NAME="quizpreviouslevel" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="3" SEQUENCE="false" NEXT="quiznextlevel" PREVIOUS="nextlevel"/>
        <FIELD NAME="quiznextlevel" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="1" SEQUENCE="false" NEXT="pointreport" PREVIOUS="quizpreviouslevel"/>
        <FIELD NAME="pointreport" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="questionmark" PREVIOUS="quiznextlevel"/>
        <FIELD NAME="questionmark" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="bookcovers" PREVIOUS="pointreport"/>
        <FIELD NAME="bookcovers" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="attemptsofday" PREVIOUS="questionmark"/>
        <FIELD NAME="attemptsofday" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="ignoredate" PREVIOUS="bookcovers"/>
        <FIELD NAME="ignoredate" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="goal" PREVIOUS="attemptsofday"/>
        <FIELD NAME="goal" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="wordsorpoints" PREVIOUS="ignoredate"/>
        <FIELD NAME="wordsorpoints" TYPE="char" LENGTH="30" NOTNULL="false" DEFAULT="words" SEQUENCE="false" NEXT="secmeass" PREVIOUS="goal"/>
        <FIELD NAME="secmeass" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="promotionstop" PREVIOUS="wordsorpoints"/>
        <FIELD NAME="promotionstop" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="99" SEQUENCE="false" NEXT="levelcheck" PREVIOUS="secmeass"/>
        <FIELD NAME="levelcheck" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="1" SEQUENCE="false" NEXT="reportwordspoints" PREVIOUS="promotionstop"/>
        <FIELD NAME="reportwordspoints" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="wordsprogressbar" PREVIOUS="levelcheck"/>
        <FIELD NAME="wordsprogressbar" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="1" SEQUENCE="false" NEXT="bookinstances" PREVIOUS="reportwordspoints"/>
        <FIELD NAME="bookinstances" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="sendmessagesaboutcheating" PREVIOUS="wordsprogressbar"/>
        <FIELD NAME="sendmessagesaboutcheating" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="1" SEQUENCE="false" NEXT="cheated_message" PREVIOUS="bookinstances"/>
        <FIELD NAME="cheated_message" TYPE="text" NOTNULL="false" SEQUENCE="false" NEXT="not_cheated_message" PREVIOUS="sendmessagesaboutcheating"/>
        <FIELD NAME="not_cheated_message" TYPE="text" NOTNULL="false" SEQUENCE="false" NEXT="checkbox" PREVIOUS="cheated_message"/>
        <FIELD NAME="checkbox" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="not_cheated_message"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="course_key" UNIQUE="false" FIELDS="course"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="reader_attempts" COMMENT="details of attempts at Reader quizzes" NEXT="reader_book_instances" PREVIOUS="reader">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="true" NEXT="uniqueid"/>
        <FIELD NAME="uniqueid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="reader" PREVIOUS="id"/>
        <FIELD NAME="reader" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="userid" PREVIOUS="uniqueid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="bookid" PREVIOUS="reader"/>
        <FIELD NAME="bookid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" COMMENT="id from reader_books table" NEXT="quizid" PREVIOUS="userid"/>
        <FIELD NAME="quizid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="attempt" PREVIOUS="bookid"/>
        <FIELD NAME="attempt" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="deleted" PREVIOUS="quizid"/>
        <FIELD NAME="deleted" TYPE="int" LENGTH="1" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="sumgrades" PREVIOUS="attempt"/>
        <FIELD NAME="sumgrades" TYPE="float" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="percentgrade" PREVIOUS="deleted"/>
        <FIELD NAME="percentgrade" TYPE="number" LENGTH="6" NOTNULL="false" SEQUENCE="false" DECIMALS="2" NEXT="passed" PREVIOUS="sumgrades"/>
        <FIELD NAME="passed" TYPE="char" LENGTH="10" NOTNULL="false" DEFAULT="false" SEQUENCE="false" NEXT="checkbox" PREVIOUS="percentgrade"/>
        <FIELD NAME="checkbox" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="timestart" PREVIOUS="passed"/>
        <FIELD NAME="timestart" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="timefinish" PREVIOUS="checkbox"/>
        <FIELD NAME="timefinish" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="timemodified" PREVIOUS="timestart"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="layout" PREVIOUS="timefinish"/>
        <FIELD NAME="layout" TYPE="text" NOTNULL="false" SEQUENCE="false" NEXT="preview" PREVIOUS="timemodified"/>
        <FIELD NAME="preview" TYPE="int" LENGTH="3" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="bookrating" PREVIOUS="layout"/>
        <FIELD NAME="bookrating" TYPE="int" LENGTH="2" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="ip" PREVIOUS="preview"/>
        <FIELD NAME="ip" TYPE="char" LENGTH="40" NOTNULL="false" SEQUENCE="false" PREVIOUS="bookrating"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="mdl_readatte_uni_uix"/>
        <KEY NAME="mdl_readatte_uni_uix" TYPE="unique" FIELDS="uniqueid" PREVIOUS="primary"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="reader_key" UNIQUE="false" FIELDS="reader" NEXT="userid_key"/>
        <INDEX NAME="userid_key" UNIQUE="false" FIELDS="userid" NEXT="bookid_key" PREVIOUS="reader_key"/>
        <INDEX NAME="bookid_key" UNIQUE="false" FIELDS="bookid" COMMENT="index on bookid field" NEXT="quizid_key" PREVIOUS="userid_key"/>
        <INDEX NAME="quizid_key" UNIQUE="false" FIELDS="quizid" NEXT="attempt_key" PREVIOUS="bookid_key"/>
        <INDEX NAME="attempt_key" UNIQUE="false" FIELDS="attempt" NEXT="deleted_key" PREVIOUS="quizid_key"/>
        <INDEX NAME="deleted_key" UNIQUE="false" FIELDS="deleted" NEXT="timefinish_key" PREVIOUS="attempt_key"/>
        <INDEX NAME="timefinish_key" UNIQUE="false" FIELDS="timefinish" PREVIOUS="deleted_key"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="reader_book_instances" COMMENT="reader_book_instances table with individual books list" NEXT="reader_books" PREVIOUS="reader_attempts">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="true" NEXT="readerid"/>
        <FIELD NAME="readerid" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="bookid" PREVIOUS="id"/>
        <FIELD NAME="bookid" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="difficulty" PREVIOUS="readerid"/>
        <FIELD NAME="difficulty" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="length" PREVIOUS="bookid"/>
        <FIELD NAME="length" TYPE="char" LENGTH="4" NOTNULL="false" DEFAULT="1" SEQUENCE="false" PREVIOUS="difficulty"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="readerid_key" UNIQUE="false" FIELDS="readerid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="reader_books" COMMENT="reader_books table retrofitted from MySQL" NEXT="reader_delays" PREVIOUS="reader_book_instances">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="true" NEXT="publisher"/>
        <FIELD NAME="publisher" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" NEXT="series" PREVIOUS="id"/>
        <FIELD NAME="series" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" NEXT="level" PREVIOUS="publisher"/>
        <FIELD NAME="level" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" NEXT="difficulty" PREVIOUS="series"/>
        <FIELD NAME="difficulty" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="name" PREVIOUS="level"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" NEXT="words" PREVIOUS="difficulty"/>
        <FIELD NAME="words" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" NEXT="genre" PREVIOUS="name"/>
        <FIELD NAME="genre" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" NEXT="fiction" PREVIOUS="words"/>
        <FIELD NAME="fiction" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" NEXT="quizid" PREVIOUS="genre"/>
        <FIELD NAME="quizid" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="image" PREVIOUS="fiction"/>
        <FIELD NAME="image" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" NEXT="length" PREVIOUS="quizid"/>
        <FIELD NAME="length" TYPE="char" LENGTH="4" NOTNULL="false" DEFAULT="1" SEQUENCE="false" NEXT="private" PREVIOUS="image"/>
        <FIELD NAME="private" TYPE="int" LENGTH="2" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="sametitle" PREVIOUS="length"/>
        <FIELD NAME="sametitle" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" NEXT="hidden" PREVIOUS="private"/>
        <FIELD NAME="hidden" TYPE="int" LENGTH="2" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="maxtime" PREVIOUS="sametitle"/>
        <FIELD NAME="maxtime" TYPE="int" LENGTH="3" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="time" PREVIOUS="hidden"/>
        <FIELD NAME="time" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" PREVIOUS="maxtime"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="difficulty_key" UNIQUE="false" FIELDS="difficulty" NEXT="genre_key"/>
        <INDEX NAME="genre_key" UNIQUE="false" FIELDS="genre" NEXT="hidden_key" PREVIOUS="difficulty_key"/>
        <INDEX NAME="hidden_key" UNIQUE="false" FIELDS="hidden" NEXT="length_key" PREVIOUS="genre_key"/>
        <INDEX NAME="length_key" UNIQUE="false" FIELDS="length" NEXT="level_key" PREVIOUS="hidden_key"/>
        <INDEX NAME="level_key" UNIQUE="false" FIELDS="level" NEXT="name_key" PREVIOUS="length_key"/>
        <INDEX NAME="name_key" UNIQUE="false" FIELDS="name" NEXT="publisher_key" PREVIOUS="level_key"/>
        <INDEX NAME="publisher_key" UNIQUE="false" FIELDS="publisher" NEXT="quizid_key" PREVIOUS="name_key"/>
        <INDEX NAME="quizid_key" UNIQUE="false" FIELDS="quizid" NEXT="sametitle_key" PREVIOUS="publisher_key"/>
        <INDEX NAME="sametitle_key" UNIQUE="false" FIELDS="sametitle" PREVIOUS="quizid_key"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="reader_delays" COMMENT="specifies the required delay interval between quizzes" NEXT="reader_goals" PREVIOUS="reader_books">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="true" NEXT="readerid"/>
        <FIELD NAME="readerid" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="groupid" PREVIOUS="id"/>
        <FIELD NAME="groupid" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="level" PREVIOUS="readerid"/>
        <FIELD NAME="level" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="delay" PREVIOUS="groupid"/>
        <FIELD NAME="delay" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="timemodified" PREVIOUS="level"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" PREVIOUS="delay"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="readerid_key" UNIQUE="false" FIELDS="readerid" COMMENT="non-unique index on readerid field" NEXT="groupid_key"/>
        <INDEX NAME="groupid_key" UNIQUE="false" FIELDS="groupid" PREVIOUS="readerid_key"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="reader_goals" COMMENT="reading goals for each Reader activity" NEXT="reader_grades" PREVIOUS="reader_delays">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="true" NEXT="readerid"/>
        <FIELD NAME="readerid" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="groupid" PREVIOUS="id"/>
        <FIELD NAME="groupid" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="level" PREVIOUS="readerid"/>
        <FIELD NAME="level" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="goal" PREVIOUS="groupid"/>
        <FIELD NAME="goal" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="timemodified" PREVIOUS="level"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" PREVIOUS="goal"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="readerid_key" UNIQUE="false" FIELDS="readerid" COMMENT="non-unique index on readid field" NEXT="groupid_key"/>
        <INDEX NAME="groupid_key" UNIQUE="false" FIELDS="groupid" PREVIOUS="readerid_key"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="reader_grades" COMMENT="reader_grades table retrofitted from MySQL" NEXT="reader_levels" PREVIOUS="reader_goals">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="true" NEXT="reader"/>
        <FIELD NAME="reader" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="userid" PREVIOUS="id"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="grade" PREVIOUS="reader"/>
        <FIELD NAME="grade" TYPE="float" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="timemodified" PREVIOUS="userid"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="grade"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="userid_key" UNIQUE="false" FIELDS="userid" NEXT="reader_key"/>
        <INDEX NAME="reader_key" UNIQUE="false" FIELDS="reader" PREVIOUS="userid_key"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="reader_levels" COMMENT="reader_levels table retrofitted from MySQL" NEXT="reader_messages" PREVIOUS="reader_grades">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="true" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="startlevel" PREVIOUS="id"/>
        <FIELD NAME="startlevel" TYPE="int" LENGTH="11" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="currentlevel" PREVIOUS="userid"/>
        <FIELD NAME="currentlevel" TYPE="int" LENGTH="11" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="readerid" PREVIOUS="startlevel"/>
        <FIELD NAME="readerid" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="nopromote" PREVIOUS="currentlevel"/>
        <FIELD NAME="nopromote" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="0" SEQUENCE="false" NEXT="promotionstop" PREVIOUS="readerid"/>
        <FIELD NAME="promotionstop" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="99" SEQUENCE="false" NEXT="goal" PREVIOUS="nopromote"/>
        <FIELD NAME="goal" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="time" PREVIOUS="promotionstop"/>
        <FIELD NAME="time" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" PREVIOUS="goal"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="userid_key" UNIQUE="false" FIELDS="userid" NEXT="startlevel_key"/>
        <INDEX NAME="startlevel_key" UNIQUE="false" FIELDS="startlevel" NEXT="currentlevel_key" PREVIOUS="userid_key"/>
        <INDEX NAME="currentlevel_key" UNIQUE="false" FIELDS="currentlevel" NEXT="readerid_key" PREVIOUS="startlevel_key"/>
        <INDEX NAME="readerid_key" UNIQUE="false" FIELDS="readerid" PREVIOUS="currentlevel_key"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="reader_messages" COMMENT="Messages displayed on Reader module view page" NEXT="reader_question_instances" PREVIOUS="reader_levels">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="true" NEXT="readerid"/>
        <FIELD NAME="readerid" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" NEXT="teacherid" PREVIOUS="id"/>
        <FIELD NAME="teacherid" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" NEXT="groupids" PREVIOUS="readerid"/>
        <FIELD NAME="groupids" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" NEXT="message" PREVIOUS="teacherid"/>
        <FIELD NAME="message" TYPE="text" NOTNULL="true" SEQUENCE="false" NEXT="timefinish" PREVIOUS="groupids"/>
        <FIELD NAME="timefinish" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" NEXT="timemodified" PREVIOUS="message"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" PREVIOUS="timefinish"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="readerid_key" UNIQUE="false" FIELDS="readerid" NEXT="timefinish_key"/>
        <INDEX NAME="timefinish_key" UNIQUE="false" FIELDS="timefinish" PREVIOUS="readerid_key"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="reader_question_instances" COMMENT="reader_question_instances table retrofitted from MySQL" NEXT="reader_check_question_id" PREVIOUS="reader_messages">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="true" NEXT="quiz"/>
        <FIELD NAME="quiz" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="question" PREVIOUS="id"/>
        <FIELD NAME="question" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" NEXT="grade" PREVIOUS="quiz"/>
        <FIELD NAME="grade" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" PREVIOUS="question"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
